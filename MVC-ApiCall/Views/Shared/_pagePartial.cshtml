@model CharacterResults


<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
        <li class="page-item disabled">
            <a class="page-link">Previous</a>
        </li>
        @for (int i = Model.Min; i < Model.Max; i++)
        {
            <li class="page-item"><a class="page-link" href="https://localhost:7034/Home/Index/?page=@i">@i</a></li>
        }

        <li class="page-item">
            <a class="page-link" href="https://localhost:7034/Home/Index/?next="+@Model.Max>Next</a>
        </li>
    </ul>
</nav>